In [30]:

class cbow(nn.Module):
    def __init__(self, vocab_size, context_size, embedding_dim, hidden_size=128):
        super(cbow, self).__init__()
        self.embedding = nn.Embedding(vocab_size, embedding_dim)
        self.fn1 = nn.Linear(context_size*emb_dim, hidden_size)
        self.fn2 = nn.Linear(hidden_size, vocab_size)
    def forward(self, x, batch_size):
        x = self.embedding(x).viw(batch_size, -1)
        x = self.fcl(x)
        x = F.relu(x)
        x = self.fn2(x)
        return x
